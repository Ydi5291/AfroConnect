{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "first-angular-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node prebuild.mjs",
    "build": "npm run prebuild ; ng build",
    "start": "ng serve",
    "env:prod": "node prebuild-prod.mjs",
    "build:prod": "npm run env:prod && ng build --configuration production && node generate-static-pages.mjs",
    "build:vercel": "ng build --configuration production --base-href /",
    "build:ghpages": "ng build --configuration production --base-href /AfroConnect/",
    "deploy:ghpages": "npm run build:ghpages && npx angular-cli-ghpages --dir=dist/first-angular-project/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:first-angular-project": "node dist/first-angular-project/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.15",
    "@angular/compiler": "19.2.15",
    "@angular/core": "19.2.15",
    "@angular/fire": "19.0.0",
    "@angular/forms": "19.2.15",
    "@angular/google-maps": "19.2.15",
    "@angular/platform-browser": "19.2.15",
    "@angular/platform-browser-dynamic": "19.2.15",
    "@angular/platform-server": "19.2.15",
    "@angular/router": "19.2.15",
    "@angular/service-worker": "19.2.15",
    "@angular/ssr": "19.2.15",
    "@capacitor/android": "^7.4.3",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^7.4.3",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.3",
    "@googlemaps/js-api-loader": "^2.0.1",
    "@stripe/stripe-js": "^8.4.0",
    "express": "^4.18.2",
    "firebase": "^11.10.0",
    "node-fetch": "^3.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.15",
    "@angular/cli": "19.2.15",
    "@angular/compiler-cli": "19.2.15",
    "@ngx-env/builder": "^20.1.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^2.0.3",
    "dotenv": "^17.2.3",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}